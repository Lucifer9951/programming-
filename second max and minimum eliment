#include <stdio.h>

int main()
{
	int array[5],i, max1,max2,min,c;

  
   printf("Enter numbers for array \n"  );

   for (c = 0; c < 5; c++){
   	scanf("%d", &array[c])
	   ;
   }
   
     max1 = array[0];

    for(i=1; i<5; i++)
    {
        if(array[i]>max1)
        {
            max1 = array[i];
        }
    }
    
    max2 = array[0];
    for (i = 1; i < 5; i++) 
	{
        if (array[i] > max2 && array[i]>max2)
            max2 = array[i];

	}
 
 
	min=array[0];
	for(i=1;i<5;i++){
	  if(array[i]<min )
	   min=array[i];	
	}	
	
	
	printf("secound maximum element is : %d\n", max2);
	printf("minimum element is : %d\n", min);  
      
}
