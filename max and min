#include <stdio.h>

void main()
{
    int arr1[5];
    int i, max,n;
		   
       printf("Input  elements in the array :\n",n);
       for(i=0;i<5;i++)
            {
	      printf("element - %d : ",i);
	      scanf("%d",&arr1[i]);
	    }


    max = arr1[0];

    for(i=1; i<5; i++)
    {
        if(arr1[i]>max)
        {
            max = arr1[i];
        }
    }
    printf("Maximum element is : %d\n", max);
}